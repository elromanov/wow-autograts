<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML/UI.xsd">
    <Frame name="AutoGratsOptionsPanel" parent="UIParent">
        <CheckButton name="AutoGratsEnableCheckbox" inherits="OptionsCheckButtonTemplate">
            <Size>
                <AbsDimension x="26" y="26"/>
            </Size>
            <Anchors>
                <Anchor point="TOPLEFT" relativeTo="AutoGratsOptionsPanel" relativePoint="TOPLEFT" x="10" y="-40"/>
            </Anchors>
            <Text>
                <FontString inherits="GameFontNormal" text="Enable AutoGrats"/>
            </Text>
            <Scripts>
                <OnLoad>
                    AutoGrats_CheckButton_OnLoad(self);
                </OnLoad>
            </Scripts>
        </CheckButton>
    </Frame>
</Ui>